{% extends 'base.twig' %}
{% block content %}
	<div
		class="content-wrapper">
		<article
			class="post-type-{{ post.post_type }}"
			id="post-{{ post.ID }}">
			<section
				class="article-content">
				{# <h1 class="article-h1">{{post.title}}</h1> #}
				<div
					class="article-body my-3">
					<div
						class="container">
						<div
							class="row g-0">
							<div
								class="col-12">
								{{ post.content }}
							</div>
						</div>
					</div>
				</div>
			</section>
		</article>
	</div>
	<!-- /content-wrapper -->
{% endblock %}
